{
  "name": "wire-number-phase-colour",
  "version": "1.0.2",
  "description": "Electrical wire number phase colour",
  "main": "server/development.js",
  "directories": {
    "test": "test",
    "client": "app",
    "server": "server",
    "production": "build"
  },
  "engines": {
    "node": "6.9.2",
    "npm": "3.10.9"
  },
  "scripts": {
    "clean": "rimraf build/",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register -r babel-polyfill -r dotenv/config -r ./test/setup.js test/**/**/**/*.js test/**/*.js",
    "start": "cross-env NODE_ENV=development nodemon -r babel-register -r babel-polyfill -r dotenv/config server/development.js",
    "prebuild": "npm run clean",
    "build:server": "webpack -p --config ./webpack.server.js --progress --color",
    "build:app": "webpack -p --config ./webpack.production.config.js --progress --color",
    "build": "cross-env NODE_ENV=production npm run build:app && npm run build:server",
    "predeploy": "npm run build",
    "lint": "eslint app/",
    "start-production": "pm2 start -i 2 --name='wireNumberPhaseColour' build/production.server.bundle.js",
    "start:demo": "cross-env NODE_ENV=production node -r dotenv/config build/production.server.bundle.js"
  },
  "author": "Sean Campbell <sean.campbell13@gmail.com> (http://seancampbellnatac.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/natac13/wire-number-phase-colour/issues"
  },
  "homepage": "https://github.com/natac13/wire-number-phase-colour#readme",
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "babel": "6.5.2",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.4",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "chai-immutable": "1.5.4",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "eslint": "2.8.0",
    "eslint-config-natac13": "1.3.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.15.0",
    "immutable-devtools": "0.0.7",
    "json-loader": "0.5.4",
    "mocha": "2.4.5",
    "node-sass": "3.4.2",
    "nodemon": "1.9.2",
    "normalize.css": "4.1.1",
    "postcss-loader": "0.8.2",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.1",
    "supertest": "1.2.0",
    "url-loader": "0.5.7",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "dependencies": {
    "axios": "0.9.1",
    "babel-cli": "6.7.5",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "bluebird": "3.3.5",
    "body-parser": "1.15.0",
    "classnames": "2.2.5",
    "compression": "1.6.1",
    "dotenv": "2.0.0",
    "express": "4.13.4",
    "history": "2.0.2",
    "hover.css": "2.1.1",
    "immutability-helper": "2.1.0",
    "immutable": "3.8.1",
    "lodash": "4.14.1",
    "modularscale-sass": "2.1.1",
    "morgan": "1.7.0",
    "path": "0.12.7",
    "ramda": "0.21.0",
    "react": "15.4.1",
    "react-addons-css-transition-group": "15.4.1",
    "react-bootstrap": "0.30.8",
    "react-dom": "15.4.1",
    "react-fa": "4.1.2",
    "react-immutable-proptypes": "1.7.1",
    "react-redux": "4.4.5",
    "react-router": "2.5.2",
    "react-router-redux": "4.0.2",
    "react-tap-event-plugin": "2.0.1",
    "recompose": "0.17.0",
    "redux": "3.6.0",
    "redux-actions": "0.9.1",
    "redux-form": "6.5.0",
    "redux-immutable": "4.0.0",
    "redux-logger": "2.6.1",
    "redux-promise": "0.5.3",
    "serve-favicon": "2.3.0",
    "webpack": "1.13.0"
  }
}
